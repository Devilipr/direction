<%@ page contentType="text/html;charset=UTF-8" %>
<%@ include file="/WEB-INF/views/include/taglib.jsp"%>
<html>
<head>
	<title>商品高拍仪</title>
	<meta name="decorator" content="default"/>
	<script type="text/javascript">
		$(document).ready(function() {
			
		});
		function page(n,s){
			$("#pageNo").val(n);
			$("#pageSize").val(s);
			$("#searchForm").submit();
        	return false;
        }
	</script>
</head>
<body>
	<ul class="nav nav-tabs">
		<li class="active"><a href="${ctx}/vjhrkmx/vjhrkmx/">入库数据</a></li>
	</ul>
	<form:form id="searchForm" modelAttribute="vjhrkmx" action="${ctx}/vjhrkmx/vjhrkmx/" method="post" class="breadcrumb form-search">
		<input id="pageNo" name="pageNo" type="hidden" value="${page.pageNo}"/>
		<input id="pageSize" name="pageSize" type="hidden" value="${page.pageSize}"/>
		<ul class="ul-form">
 			<li><label>商品名称：</label>
				<form:input path="spmch" htmlEscape="false" maxlength="255" class="input-medium"/>
			</li>
			<li><label>批次：</label>
				<form:input path="pici" htmlEscape="false" maxlength="255" class="input-medium"/>
			</li>
 			<li class="btns"><input id="btnSubmit" class="btn btn-primary" type="submit" value="查询"/></li>
			<li class="clearfix"></li>
		</ul>
	</form:form>
	<sys:message content="${message}"/>
	<table id="alltable" class="table table-striped table-bordered table-condensed">
		<thead>
			<tr>
				<th>单据编号</th>
				<th>单位编号</th>
				<th>商品编号</th>
				<th>单位名称</th>
				<th>商品名称</th>
				<th>规格</th>	
				<th>生产厂商</th>
				<th>批次</th>
				<th>批号</th>
				<th>数量</th>
				<th>含税金额</th>
				<th>日期</th>
				<shiro:hasPermission name="vjhrkmx:vjhrkmx:view"><th>操作</th></shiro:hasPermission>
			</tr>
		</thead>
		<tbody>
		<c:forEach items="${page.list}" var="vjhrkmx">
			<tr>
				<td>
					${vjhrkmx.djbh}
				</td>
				<td>
					${vjhrkmx.danwbh}
				</td>
				<td>
					${vjhrkmx.spbh}
				</td>
				<td>
					${vjhrkmx.dwmch}
				</td>
				<td>
					${vjhrkmx.spmch}
				</td>
				<td>
					${vjhrkmx.shpgg}
				</td>
				<td>
					${vjhrkmx.shengccj}
				</td>
				<td>
					${vjhrkmx.pici}
				</td>
				<td>
					${vjhrkmx.pihao}
				</td>
				<td>
					${vjhrkmx.shl}
				</td>
				<td>
					${vjhrkmx.hsje}
				</td>
				<td>
					${vjhrkmx.rq}
				</td>
				
				<shiro:hasPermission name="vjhrkmx:vjhrkmx:view">
				<td>
					<a href="${ctx}/vjhrkmx/vjhrkmx/form?id=${vjhrkmx.pici}">拍摄</a>
    				<a href="${ctx}/delivery/delivery/form?id=${vjhrkmx.pici}">详情</a>				
    			</td>
    			</shiro:hasPermission>
			</tr>
		</c:forEach>
		</tbody>
	</table>
	<div class="pagination">${page}</div>
</body>
</html>